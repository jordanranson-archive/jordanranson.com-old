<div class="wrapper stream">
    <div class="content">

        {{#each months}}
        <h2>{{month}} <span>{{year}}</span></h2>

        <div>

            {{#each assets}}

                {{#if important}}
                    {{#if asset.media}}
                    <section class="article">
                        <div class="media left" style="background-image:url('{{asset.media.0.mediaUrl}}')"></div>
                        <article class="right">
                            <h3>{{title}}</h3>
                            <p>{{{body}}}</p>
                            <footer>
                                <!--<a href="{{href}}" target="_blank">Check it out</a>-->
                                <var>{{day}}</var>
                            </footer>
                        </article>
                    </section>
                    {{else}}
                    <section class="article">
                        <article>
                            <h3>{{title}}</h3>
                            <p>{{{body}}}</p>
                            <footer>
                                <!--<a href="{{href}}" target="_blank">Check it out</a>-->
                                <var>{{day}}</var>
                            </footer>
                        </article>
                    </section>
                    {{/if}}
                {{/if}}

                {{#if verified}}
                    {{#if asset.media}}
                        <section class="tweet">
                            <div class="media left" style="background-image:url('{{asset.media.0.mediaUrl}}')"></div>
                            <article class="right">
                                <h3>via <em>{{asset.socialType}}</em></h3>
                                <p>{{{asset.longText}}}</p>
                                <footer>
                                    <var>{{day}}</var>
                                </footer>
                            </article>
                        </section>
                    {{else}}
                        <section class="tweet">
                            <article>
                                <h3>via <em>{{asset.socialType}}</em></h3>
                                <p>{{{asset.longText}}}</p>
                                <footer>
                                    <var>{{day}}</var>
                                </footer>
                            </article>
                        </section>
                    {{/if}}
                {{/if}}

                {{#if normal}}
                    {{#if asset.media}}
                    <section class="tweet">
                        <div class="media left" style="background-image:url('{{asset.media.0.mediaUrl}}')"></div>
                        <article class="right">
                            <h3>@{{asset.authorDisplayName}}</h3>
                            <p>{{{asset.longText}}}</p>
                            <footer>
                                <var>{{day}}</var>
                            </footer>
                        </article>
                    </section>
                    {{else}}
                    <section class="tweet">
                        <article>
                            <h3>@{{asset.authorDisplayName}}</h3>
                            <p>{{{asset.longText}}}</p>
                            <footer>
                                <var>{{day}}</var>
                            </footer>
                        </article>
                    </section>
                    {{/if}}
                {{/if}}

                {{#if highlight}}
                    {{#if asset.media}}
                    <section class="media">
                        <div class="media" style="background-image:url('{{asset.media.0.mediaUrl}}')">
                            <var class="stamped">{{day}}</var>
                        </div>
                    </section>
                    {{else}}
                    <section class="tweet">
                        <article>
                            <h3>@{{asset.authorDisplayName}}</h3>
                            <p>{{{asset.longText}}}</p>
                            <footer>
                                <var>{{day}}</var>
                            </footer>
                        </article>
                    </section>
                    {{/if}}
                {{/if}}

                <hr>

            {{/each}}

        </div>
        {{/each}}

        {{! <section class="gallery">
            <div class="media"></div>
